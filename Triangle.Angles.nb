  RowBox[{
   RowBox[{"PolygonACentroid2d", "[", "poly_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "i", ",", "n", ",", "p1", ",", "pi", ",", "pi1", ",", "cent", ",", 
