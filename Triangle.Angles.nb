Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"InsideTriQ", "[", 
    RowBox[{"p_", ",", 
     RowBox[{"{", 
      RowBox[{"a_", ",", "b_", ",", "c_"}], "}"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "tri", ",", "plane", ",", "v0", ",", "v1", ",", "v2", ",", "d00", ",", 
       "d01", ",", "d11", ",", "d20", ",", "d21", ",", "denom", ",", "v", ",",
        "w", ",", "u"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"tri", "=", 
       RowBox[{"{", 
        RowBox[{"a", ",", "b", ",", "c"}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"plane", "=", 
       RowBox[{"Tri2Plane", "[", "tri", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "If", " ", "not", " ", "even", " ", "on", " ", "the", " ", "plane", 
        " ", "of", " ", "that", " ", "face"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"!", 
         RowBox[{"PtOnPlaneQ", "[", 
          RowBox[{"p", ",", "plane"}], "]"}]}], ",", 
        RowBox[{"Return", "[", "False", "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{"DebugTri", ",", 
        RowBox[{"Print", "[", "\"\<InsideTriQ:p on plane\>\"", "]"}]}], "]"}],
       ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Barycentric", " ", "coordinates", " ", "to", " ", "determine", " ", 
        "if", " ", "inside"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"v0", "=", 
       RowBox[{"b", "-", "a"}]}], ";", 
      RowBox[{"v1", "=", 
       RowBox[{"c", "-", "a"}]}], ";", 
      RowBox[{"v2", "=", 
       RowBox[{"p", "-", "a"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"d00", "=", 
       RowBox[{"v0", ".", "v0"}]}], ";", 
      RowBox[{"d01", "=", 
       RowBox[{"v0", ".", "v1"}]}], ";", 
      RowBox[{"d11", "=", 
       RowBox[{"v1", ".", "v1"}]}], ";", 
      RowBox[{"d20", "=", 
       RowBox[{"v2", ".", "v0"}]}], ";", 
      RowBox[{"d21", "=", 
       RowBox[{"v2", ".", "v1"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"denom", "=", 
       RowBox[{
        RowBox[{"d00", " ", "d11"}], "-", 
        RowBox[{"d01", " ", "d01"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"FuzzZero", "[", "denom", "]"}], ",", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{"\"\<Error in InsideTriQ:denom=\>\"", ",", "denom"}], "]"}],
          ";", 
         RowBox[{"Abort", "[", "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"v", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"d11", " ", "d20"}], "-", 
          RowBox[{"d01", " ", "d21"}]}], ")"}], "/", "denom"}]}], ";", 
      RowBox[{"w", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"d00", " ", "d21"}], "-", 
          RowBox[{"d01", " ", "d20"}]}], ")"}], "/", "denom"}]}], ";", 
      RowBox[{"u", "=", 
       RowBox[{"1.", "-", "v", "-", "w"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{"DebugTri", ",", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\<InsideTriQ:{u,v,w}=\>\"", ",", 
          RowBox[{"{", 
           RowBox[{"u", ",", "v", ",", "w"}], "}"}]}], "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"0", "<", "u"}], ")"}], "&&", 
         RowBox[{"(", 
          RowBox[{"u", "<", "1"}], ")"}], "&&", 
         RowBox[{"(", 
          RowBox[{"0", "<", "v"}], ")"}], "&&", 
         RowBox[{"(", 
          RowBox[{"v", "<", "1"}], ")"}], "&&", 
         RowBox[{"(", 
          RowBox[{"0", "<", "w"}], ")"}], "&&", 
         RowBox[{"(", 
          RowBox[{"w", "<", "1"}], ")"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
          StyleBox["Strictly",
           FontSlant->"Italic"], " ", "inside"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"Return", "[", "True", "]"}], ",", "\[IndentingNewLine]", 
        RowBox[{"Return", "[", "False", "]"}]}], "\[IndentingNewLine]", "]"}],
       ";"}]}], 
    RowBox[{"(*", "If", "*)"}], "\[IndentingNewLine]", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
